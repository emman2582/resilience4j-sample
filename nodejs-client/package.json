{
  "name": "resilience4j-nodejs-client",
  "version": "1.0.0",
  "description": "NodeJS client for Resilience4j Service A microservices",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "jest --config config/jest.config.js",
    "test:performance": "node src/performance-test.js",
    "start:aws": "NODE_ENV=aws node src/index.js",
    "test:aws": "NODE_ENV=aws npm run test:performance",
    "test:all": "npm test && npm run test:performance",
    "setup": "./scripts/setup.sh",
    "test:comprehensive": "./scripts/test-all.sh",
    "cleanup": "./scripts/cleanup.sh"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "autocannon": "^7.15.0",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}